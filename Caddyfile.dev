{
	auto_https off
	admin off
}

{$API_HOST:-api.local.einstore.pro} {
	reverse_proxy 127.0.0.1:{$API_PORT:-8080}
}

{$ADMIN_HOST:-admin.local.einstore.pro} {
	reverse_proxy 127.0.0.1:{$ADMIN_PORT:-5173}
}

{$WEB_HOST:-local.einstore.pro} {
	root * Web
	encode gzip
	try_files {path} /index.html
	file_server
}
