{
	"name": "Boost Enterprise AppStore",
	"description": "Enterprise appstore for easy project deployment",
	"repository": "https://github.com/LiveUI/Boost",
	"website": "https://www.boostappstore.com",
	"keywords": [
		"boost",
		"hockeyapp",
		"fabric",
		"ios",
		"android",
		"enterprise",
		"appstore"
	],
    "env": {
        "JWT_SECRET": {
            "description": "Your secret to authenticate JWT tokens, can be a set of random characters.",
            "required": true
        },
        "DB_HOST": {
            "description": "PostgreSQL database host.",
            "value": "localhost",
            "required": true
        },
        "DB_PORT": {
            "description": "PostgreSQL database host.",
            "value": "5432",
            "required": true
        },
        "DB_USER": {
            "description": "PostgreSQL database host.",
            "value": "boost",
            "required": true
        },
        "DB_PASSWORD": {
            "description": "PostgreSQL database password.",
            "required": true
        },
        "DB_NAME": {
            "description": "PostgreSQL database name.",
            "value": "boost",
            "required": true
        }
    },
	"buildpacks": [{
		"url": "https://github.com/LiveUI/heroku-buildpack"
	}]
}
